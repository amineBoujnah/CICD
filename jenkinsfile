pipeline {
    agent any

    stages {
        stage("clone code") {
            steps {
                script {
                    // Let's clone the source
                    git 'https://github.com/amineBoujnah/CICD';
                }
            }
        }
        }
    stages {
        stage('Build') {
            steps {
              bat  'mvn clean install'
            }
        }
        stage('Test') {
            steps {
                bat 'mvn test'
            }
        }
        stage('Deploy') {
            steps {
                bat 'echo deploy'
            }
        }
    }
}
